(format higher-order)
(sort A)
(sort B)
(sort C)
(sort SAB)
(sort arrAB)
(fun app (-> arrAB A B))
(fun case (-> SAB (-> A C) (-> B C) C))
(fun inl (-> A SAB))
(fun inr (-> B SAB))
(fun lam (-> (-> A B) arrAB))
(rule (app (lam F) Y) (F Y))
(rule (lam (lambda ((y A)) (app U y)) U)
(rule (case (inl V) I J) (I V))
(rule (case (inr X1) Z1 G1) (G1 X1))
