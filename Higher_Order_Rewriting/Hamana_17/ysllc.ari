(format higher-order)
(sort BGa)
(sort BGaBGb)
(sort BGb)
(sort a)
(sort ab)
(sort b)
(fun abs (-> (-> a b) ab))
(fun app (-> ab a b))
(fun appBG (-> BGaBGb BGa BGb))
(fun banga (-> a BGa))
(fun let (-> BGa (-> a BGb) BGb))
(rule (app (abs F) Y) (F Y))
(rule (abs (lambda ((y a)) (app U y)) U)
(rule (let (banga V) I) (I V))
(rule (let P (lambda ((y a)) (appBG X1 (banga y))) (appBG X1 P))
