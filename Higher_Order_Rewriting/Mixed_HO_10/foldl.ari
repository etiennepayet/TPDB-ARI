(format higher-order)
(sort list)
(sort nat)
(fun O nat)
(fun cons (-> nat list list))
(fun foldl (-> (-> nat nat nat) nat list nat))
(fun nil list)
(fun plus (-> nat nat nat))
(fun plusc (-> nat nat nat))
(fun sum (-> list nat))
(rule (foldl F x nil) x)
(rule (foldl F x (cons y l)) (foldl F (F x y) l))
(rule plusc (lambda ((x nat) (y nat)) (plus x y))
(rule (sum l) (foldl plusc O l))
