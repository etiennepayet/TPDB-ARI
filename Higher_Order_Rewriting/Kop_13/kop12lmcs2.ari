(format higher-order)
(sort bool)
(sort funlist)
(sort nat)
(sort string)
(fun cons (-> (-> nat nat) funlist funlist))
(fun false bool)
(fun head (-> funlist nat nat))
(fun if (-> bool (-> nat string) (-> nat string) nat string))
(fun nil funlist)
(fun s (-> nat nat))
(fun tail (-> funlist funlist))
(fun test (-> (-> nat nat) bool))
(fun true bool)
(rule (if true F Z) F)
(rule (if false F Z) Z)
(rule (test (lambda ((y nat)) (s y)) true)
(rule (head (cons G V)) G)
(rule (tail (cons G V)) V)
