(format higher-order)
(sort list)
(sort nat)
(fun O nat)
(fun cons (-> nat list list))
(fun foldr (-> (-> nat nat nat) nat list nat))
(fun length (-> list nat))
(fun nil list)
(fun s (-> nat nat))
(rule (foldr F Y nil) Y)
(rule (foldr G V (cons W P)) (G W (foldr G V P)))
(rule (length X1) (foldr (lambda ((%G nat) (%F nat)) (s %F) O X1))
